{
    "name": "acquia/blted8",
    "description": "Example project used to create new projects with BLT.",
    "license": "GPL-2.0",
    "repositories": {
        "blt": {
            "type": "path",
            "url": "../blt",
            "options": {
                "symlink": true
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "acquia/blt": "*@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
